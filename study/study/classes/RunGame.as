import mx.utils.Delegate;import mx.transitions.Tween;//class RunGame extends LZGame {	private var knowItBtn:MovieClip;	private var notSureBtn:MovieClip;	public var cardManager:CardManager;		public function RunGame(main:MovieClip) {		super(main);		setInstance(this);				knowItBtn = main.attachMovie("knowIt", "knowItDropZone", main.getNextHighestDepth(), {_x: 26, _y: 30});		notSureBtn = main.attachMovie("notSure", "notSureDropZone", main.getNextHighestDepth(), {_x: 410, _y: 30});				cardManager = new CardManager();		cardManager.createCards(questionList.getQArray());		cardManager.nextCard();				knowItBtn.onRelease = Delegate.create(cardManager, cardManager.knowIt);		notSureBtn.onRelease = Delegate.create(cardManager, cardManager.notSure);	}		public function destroyButtons():Void {		knowItBtn.enabled = false;		notSureBtn.enabled = false;		new Tween(knowItBtn, "_alpha", mx.transitions.easing.Regular.easeIn, 100, 0, 2, true);		var twn:Tween = new Tween(notSureBtn, "_alpha", mx.transitions.easing.Regular.easeIn, 100, 0, 2, true);		twn.onMotionFinished = Delegate.create(this, deleteButtons);	}	private function deleteButtons():Void {		removeMovieClip(knowItBtn);		removeMovieClip(notSureBtn);	}}