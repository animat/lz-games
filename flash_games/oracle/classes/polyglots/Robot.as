import mx.utils.Delegate;//class Robot extends Lemming {		public function Robot(man:LemmingManager, index:Number) {		super(man, index);		clip = LZGame.getStage().attachMovie("robot", "lemming"+index, LZGame.getStage().getNextHighestDepth());		clip._x = 40+random(60);		clip._y = 360 + index;		destination = clip._x;				runFX = clip.attachMovie("_robot.mp3", "_robot.mp3", clip.getNextHighestDepth());				hp = 4;		speed = 3+random(3);				clip.onEnterFrame = Delegate.create(this, updateDetails);	}	}