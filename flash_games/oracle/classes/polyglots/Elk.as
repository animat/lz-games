import mx.utils.Delegate;//class Elk extends Lemming {		public function Elk(man:LemmingManager, index:Number) {		super(man, index);		clip = LZGame.getStage().attachMovie("elk", "lemming"+index, LZGame.getStage().getNextHighestDepth());		clip._x = 40+random(60);		clip._y = 360 + index;		destination = clip._x;				runFX = clip.attachMovie("_elk.mp3", "_elk.mp3", clip.getNextHighestDepth()); 				hp = 4;		speed = 3+random(3);				clip.onEnterFrame = Delegate.create(this, updateDetails);	}	}