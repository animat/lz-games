<?xml version="1.0" encoding="utf-8"?>
<xml>
	<format gamename="Fishing">
		<attribute name="english" interactive="true" />
		<attribute name="lang" interactive="true" />
	</format>
	
	<description>
		<text>Match the French word to the English word!</text>
		<keyword>keyword #1</keyword>
		<keyword>keyword #2</keyword>
	</description>
	
	<gamedata>
		<node>
			<question name="english" type="image" content="media/image/food/water.swf" />
			<response name="lang" type="text" content="glass of water" />
		</node>
		<node>
			<question name="english" type="text" content="how are you?" />
			<response name="lang" type="text" content="I am good" />
		</node>
		<node>
			<question name="english" type="text" content="What is your favorite color?" />
			<response name="langl" type="text" content="red" />
		</node>
	</gamedata>
	
	<gamedata>
	</gamedata>
	
	<language>French</language>
</xml>