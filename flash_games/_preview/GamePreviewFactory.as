class GamePreviewFactory {	//	public function GamePreviewFactory() {}	//	public static function createGamePreview(gameName:String, clip:MovieClip, path:String):GamePreview {		switch (gameName) {			case "alphabetTree":				return new AlphabetTreePreview(gameName, clip, path);				break;			case "bathTubBubbles":				return new BathTubBubblesPreview(gameName, clip, path);				break;			case "cliffhanger":				return new CliffhangerPreview(gameName, clip, path);				break;			case "downfall":				return new DownfallPreview(gameName, clip, path);				break;			case "ferrisWheel":				return new FerrisWheelPreview(gameName, clip, path);				break;			case "gardenGrows":				return new GardenGrowsPreview(gameName, clip, path);				break;			case "leapFrog":				return new LeapFrogPreview(gameName, clip, path);				break;			case "mantis":				return new MantisPreview(gameName, clip, path);				break;			case "numberPop":				return new NumberPopPreview(gameName, clip, path);				break;			case "prepositionPower":				return new PrepositionPowerPreview(gameName, clip, path);				break;			case "quizShow":				return new QuizShowPreview(gameName, clip, path);				break;			case "sentenceSwiper":				return new SentenceSwiperPreview(gameName, clip, path);				break;			case "splat":				return new SplatPreview(gameName, clip, path);				break;			case "swish":				return new SwishPreview(gameName, clip, path);				break;		}	}}