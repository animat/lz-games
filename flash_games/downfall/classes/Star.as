import mx.utils.Delegate;//class Star {	private var clip;	private var speed;	private var paused:Boolean;	//	public function Star(clip:MovieClip) {		this.clip = clip;		//		clip._x = Math.ceil(Math.random() * 600);		clip._y = Math.ceil(Math.random() * 400);		clip._xscale = clip._yscale = 50 + Math.ceil(Math.random() * 50);		clip._alpha = 50 + Math.ceil(Math.random() * 50);		//		speed = (clip._alpha < 75) ? (3 + Math.random() * 2) : 3;		paused = false;		//		clip.onEnterFrame = Delegate.create(this, moveStar);	}	private function moveStar():Void {		if (!paused) {			clip._y += speed;			if (clip._y > 400) {				clip._y = 0;				clip._x = Math.ceil(Math.random() * 600);			}		}	}	public function pauseStar():Void {		paused = true;	}	public function resumeStar():Void {		paused = false;	}}