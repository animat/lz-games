import mx.utils.Delegate;//class RunGame extends LZGame implements gametypes.MatchingGame {	//	var brickManager:BrickManager;	var ball:Ball;	//	public function RunGame(main:MovieClip) {		super(main);		setInstance(this);		//		brickManager = new BrickManager(questionList.getQArray()); 		ball = new Ball();		brickManager.setBallClip(ball.getClip());		brickManager.checkForBallCollisions();				nextQuestion(false);	}	public function nextQuestion(proceed:Boolean):Void {		if (proceed) {			trace("moving on...");		}		if (questionList.getQuestionsLeft() == 0) {			winGame();			trace("you did it! you won!");			return;		}		refreshQDisplay();	}	public function refreshQDisplay():Void {		ball.showQuestion();	}	public function evaluate(attempt:Object):Void {		var rndm:Number = Math.random() * 100;		if (rndm > 50) {			correctAnswer();		} else {			incorrectAnswer();		}	}	public function correctAnswer():Void {		brickManager.tagCurrentBrick();	}	public function incorrectAnswer():Void {		ball.screwball();	}	public function winGame():Void {		trace("YAY! You win.");		main.gotoAndStop("win");	}		public function getScore():String {		return "";	}}