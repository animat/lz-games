import mx.utils.Delegate;//class Flora extends Lemming {		public function Flora(man:LemmingManager, index:Number) {		super(man, index);		clip = LZGame.getStage().attachMovie("flora", "lemming"+index, LZGame.getStage().getNextHighestDepth());		clip._x = 40+random(60);		clip._y = 360 + index;		destination = clip._x;				hp = 2;		speed = 4+random(2);				clip.onEnterFrame = Delegate.create(this, runToDestination);	}	}