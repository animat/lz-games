import mx.utils.Delegate;//class Squishy extends Lemming {		public function Squishy(man:LemmingManager, index:Number) {		super(man, index);		clip = LZGame.getStage().attachMovie("squishy", "lemming"+index, LZGame.getStage().getNextHighestDepth());		clip._x = 40+random(60);		clip._y = 360 + index;		destination = clip._x;				runFX = clip.attachMovie("_squishy.mp3", "_squishy.mp3", clip.getNextHighestDepth());				hp = 3;		speed = 2+random(3);				clip.onEnterFrame = Delegate.create(this, updateDetails);	}	}