import mx.utils.Delegate;//class Zap extends Lemming {		public function Zap(man:LemmingManager, index:Number) {		super(man, index);		clip = LZGame.getStage().attachMovie("zap", "lemming"+index, LZGame.getStage().getNextHighestDepth());		clip._x = 40+random(60);		clip._y = 360 + index;		destination = clip._x;				runFX = clip.attachMovie("_zap.mp3", "_zap.mp3", clip.getNextHighestDepth());				hp = 3;		speed = 2+random(5);				clip.onEnterFrame = Delegate.create(this, updateDetails);	}	}