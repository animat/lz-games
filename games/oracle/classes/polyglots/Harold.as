import mx.utils.Delegate;//class Harold extends Lemming {		public function Harold(man:LemmingManager, index:Number) {		super(man, index);		clip = LZGame.getStage().attachMovie("harold", "lemming"+index, LZGame.getStage().getNextHighestDepth());		clip._x = 40+random(60);		clip._y = 360 + index;		destination = clip._x;				hp = 5;		speed = 2+random(3);				clip.onEnterFrame = Delegate.create(this, runToDestination);	}	}