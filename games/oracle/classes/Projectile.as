import mx.transitions.Tween;import mx.utils.Delegate;//class Projectile {	private var manager:ProjectileManager;	private var clip:MovieClip;	private var num:Number;	private var bounced:Boolean;	private var speed:Number;		public function Projectile(man:ProjectileManager, index:Number, xPos:Number) {		manager = man;		num = random(2) + 1;		clip = LZGame.getStage().attachMovie("projectile"+num, "projectile"+index, LZGame.getStage().getNextHighestDepth());		clip._x = xPos;		clip._y = 0;				bounced = false;				speed = 2+random(4);				clip.onEnterFrame = Delegate.create(this, fall);	}		private function fall():Void {		clip._y += speed;		speed++;		if (clip._y > 370) {			bounce();		}	}		private function bounce():Void {		delete clip.onEnterFrame;		LZGame.getInstance().checkForCollisions(clip);		clip.gotoAndPlay("bounce");	}	private function destroyClip():Void {		removeMovieClip(clip);	}}