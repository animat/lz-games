import mx.transitions.Tween;import flash.filters.GlowFilter;//class GroceryList {	//	private var clip:MovieClip;	private var checkListItems:Array;	private var highlightFiltersArray:Array;	//	public function GroceryList(holder:MovieClip, questions:Array, maxItemCount:Number) {		clip = holder.attachMovie("paper", "paper", holder.getNextHighestDepth());		clip._alpha = 75;		clip._x = 435;		new Tween(clip, "_y", mx.transitions.easing.Elastic.easeOut, 400, 40, 3, true);		checkListItems = new Array();		highlightFiltersArray = new Array(new GlowFilter(0x6C89F9));		displayQuestions(questions, maxItemCount);	}	private function displayQuestions(questions:Array, maxItemCount:Number):Void {		var xPos:Number = 10;		var yPos:Number = 5;		var totalFoodItems = Math.min(maxItemCount, questions.length);		for (var i:Number = 0; i < totalFoodItems; i++) {			var mc:MovieClip = clip.attachMovie("checkListItem", "checkListItem_"+i, clip.getNextHighestDepth());			mc.displayStr = questions[i].getStimulus().getContent();			mc._x = xPos;			mc._y = yPos;			yPos += mc._height;			mc.checked = false;			checkListItems.push(mc);		}	}	public function checkItem(index:Number):Void {		checkListItems[index].gotoAndPlay("checked");		checkListItems[index].checked = true;	}	public function highlightCheckListItem(index:Number):Void {		if (checkListItems[index - 1]) {			checkListItems[index - 1].filters = [];		}		checkListItems[index].filters = highlightFiltersArray;	}	public function resetList():Void {		for (var i:Number = 0; i < checkListItems.length; i++) {			if (checkListItems[i].checked) {				checkListItems[i].gotoAndPlay("unchecked");				checkListItems[i].checked = false;			} else {				if (checkListItems[i].filters != []) {					checkListItems[i].filters = [];				}			}		}	}	public function destroy():Void {		removeMovieClip(clip);	}	//	public function getCheckListItemByIndex(index:Number):String {		return checkListItems[index].displayStr;	}	public function getTotalQNum():Number {		return checkListItems.length;	}}