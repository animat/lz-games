class Question {	private var qText:String, qLang:String;		public function Question(engText:String, langText:String) {		qText = engText;		qLang = langText;	}	public function getEngText():String {		return qText;	}	public function setEngText(newEng:String):Void {		qText = newEng;	}	public function getLangText():String {		return qLang;	}	public function setLangText(newLang:String):Void {		qLang = newLang;	}}