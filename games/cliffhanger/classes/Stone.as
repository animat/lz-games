import mx.transitions.Tween;//class Stone {	private var clip:MovieClip;	private var targetY:Number;	private var stimDisplay:StimulusDisplay;	//	public function Stone(holder:MovieClip, stim:Stimulus, index:Number, xx:Number, yy:Number) {		clip = holder.attachMovie("stone", "stone_"+index, holder.getNextHighestDepth());		clip._x = xx;		clip._y = -50;		targetY = yy;				stimDisplay = new StimulusDisplay(clip, "stimDisplay", stim);		stimDisplay.setTextFormat(new TextFormat("Verdana", 14, 0x412514));		stimDisplay.setPos(22, 29);				dropStone();	}	public function dropStone():Void {		new Tween(clip, "_y", mx.transitions.easing.Bounce.easeOut, clip._y, targetY, 1.3, true);	}}