class CliffhangerPreview extends GamePreview {	//	public function CliffhangerPreview(gameName:String, clip:MovieClip, path:String) {		super(gameName, clip, path);	}	public function loadSwf():Void {		clip.loadMovie(gameName+"-new.swf");	}	public function cleanUp():Void {		if (clip.runGame != undefined) {			clearInterval(delay);			setInterval(this, "commitLeftDefault", 1500);		}	}	public function commitLeftDefault():Void {		clearInterval(delay);		clip.runGame.treeManager.defaultCommitToTreeBranch("left");		setInterval(this, "correctAnim", 2000);	}	public function correctAnim():Void {		clearInterval(delay);		clip.runGame.correctAnswer();		delay = setInterval(this, "commitLeftDefault2", 3000);	}	public function commitLeftDefault2():Void {		clearInterval(delay);		clip.runGame.treeManager.defaultCommitToTreeBranch("left");		setInterval(this, "incorrectAnim", 2000);	}	public function incorrectAnim():Void {		clearInterval(delay);		clip.runGame.incorrectAnswer();		delay = setInterval(this, "finish", 3000);	}	public function finish():Void {		clearInterval(delay);		clip.runGame.winGame();	}}