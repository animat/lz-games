import tools.Engine;class Duck {        private var engText;		private var langText;		private var duck:Engine;		private var diff:Number;        function Duck(mc:MovieClip, langT:String, engT:String, dir:Number) {				this.engText = engT;                this.langText = langT;				mc.langText = this.langText; // This here seems like a roundabout way of doing this...				mc.engText = this.engText // This too... this can't be right...				duck = new Engine(mc, dir);        }				public function getDuckMC():MovieClip {			return duck.getEngineClip();		}          // Get and set duck's private eng and langText vars        public function get eng():String {                return this.engText;        }        public function set eng(engT:String):Void {                this.engText = engT;        }        public function get lang():String {                return this.langText;        }        public function set lang(langT:String):Void {                this.langText = langT;        } }