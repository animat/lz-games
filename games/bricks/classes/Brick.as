class Brick {	private var clip:MovieClip;	private var question:Question;	private var stimDisplay:BrickStimDisplay;	public function Brick(question:Question, index:Number) {		clip = LZGame.getStage().attachMovie("brick", "brick_"+index, LZGame.getStage().getNextHighestDepth());		stimDisplay = new BrickStimDisplay(clip, "stimDisplay", question.getStimulus());		stimDisplay.centerInClip();	}		public function setPos(xx:Number, yy:Number):Void {		clip._x = xx;		clip._y = yy;	}}