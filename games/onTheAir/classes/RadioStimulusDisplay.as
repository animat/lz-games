class RadioStimulusDisplay extends StimulusDisplay {	private var spkrs:MovieClip;	public function RadioStimulusDisplay(target:MovieClip, id:String, stim:Stimulus, speakers:MovieClip) {		super(target, id, stim);		spkrs = speakers;	}		private function playAudioMC():Void {		spkrs.gotoAndPlay("audio-playing");	}	private function stopAudioMC():Void {		spkrs.gotoAndStop("static");	}}