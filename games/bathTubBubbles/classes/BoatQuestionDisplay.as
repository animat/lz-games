import mx.utils.Delegate;import mx.transitions.Tween;class BoatQuestionDisplay extends StimulusDisplay {		public function BoatQuestionDisplay(target:MovieClip, id:String, stim:Stimulus) {		super(target, id, stim);	}		public function throwInTheAir():Void {		var twn:Tween = new Tween(clip, "_y", mx.transitions.easing.None.easeNone, clip._y, clip._y - 350, .35, true);		twn.onMotionFinished = Delegate.create(this, fallBackDown);	}		private function fallBackDown():Void {		new Tween(clip, "_y", mx.transitions.easing.None.easeNone, clip._y, clip._y + 350, .6, true);	}}