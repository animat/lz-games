	import com.fonetag.events.Broadcaster; 	import com.fonetag.events.IBroadcastable;	class com.fonetag.events.Element extends MovieClip implements IBroadcastable {		private var messenger:Broadcaster;		function Element() {			messenger = Broadcaster.getInstance();		}				/*	 	        IBroadcastable implementation	 	        */	public function addEventListener(eventType:String, scope:Object, handlerFunction:String):Void {		messenger.addEventListener(eventType, scope, handlerFunction);	}	public function broadcastEvent(eventType:String, dataObject:Object):Void {		messenger.dispatchEvent(eventType, dataObject);	}	public function removeEventListener(eventType:String, scope:Object, handlerFunction:String):Void {		messenger.removeEventListener(eventType, scope, handlerFunction);	}}